<!-- sections/theme__header.liquid -->
{% comment %}
{% endcomment %}

{% capture desktop_logo %}
  <div class="md:py-4 flex items-center max-w-xs py-2
    {% if section.settings.bar_layout_desktop == "center-below" %}justify-center{% endif %}">

    {% capture logo_size_light %}
      height: {{ section.settings.logo_desktop_height }}px;
      width: {{ section.settings.logo_desktop_height | times: section.settings.logo_light.aspect_ratio }}px;
    {% endcapture %}

    {% if section.settings.logo_light %}
      {% comment %} if logo is there {% endcomment %}
      <a class="!inline-block relative flex items-center hidden"  
        style="{{ logo_size_light }}"
        href="{{ routes.root_url }}" 
        rel="nofollow"
        title="{{ shop.name }}">
        {{ section.settings.logo_light 
          | image_url: width: 400 
          | image_tag: class: 'w-full h-full !bg-transparent object-contain', alt: section.settings.logo_light.alt }}
      </a>
      
    {% else %}
      {% comment %} if not, we serve text {% endcomment %}
      <a class="!inline-block relative flex items-center no-underline whitespace-nowrap"  
        href="{{ routes.root_url }}" 
        rel="nofollow"
        title="{{ shop.name }}">
        {{ shop.name }}
      </a>

    {% endif %}
  </div>
{% endcapture %}

{% capture desktop_navigation %}
  <div class="flex flex-wrap items-center
    {% if settings.type_header == 'body' %}type__body{% endif %}
    {% if settings.type_header == 'heading' %}type__heading{% endif %}
    {% if settings.type_header == 'navigation' %}type__nav{% endif %}
    {{ settings.type_header_capitilization }}
    {% if section.settings.bar_layout_desktop == "center-below" %}w-full justify-center{% endif %}
    {% if section.settings.bar_layout_desktop == "left-below" %}w-full{% endif %}
    {% if section.settings.bar_layout_desktop == "left-inline" %}pl-6 flex-grow{% endif %}">   

    {%- for link in section.settings.dropdown_links.links -%}
      <div class="flex-shrink">
        <a 
          href="{{ link.url }}"
          onclick=""
          class="header__link h-full flex items-center mr-2 block no-underline
          {% if settings.type_header == 'body' %}type__body{% endif %}
          {% if settings.type_header == 'heading' %}type__heading{% endif %}
          {% if settings.type_header == 'navigation' %}type__nav{% endif %}
          {{ settings.type_header_capitilization }}
          {% if section.settings.bar_layout_desktop == "center-below" %}px-4 md:py-4 py-2 type--small{% endif %}
          {% if section.settings.bar_layout_desktop == "left-below" %}pr-4 pt-2 pb-4 type--small{% endif %}
          {% if section.settings.bar_layout_desktop == "left-inline" %}px-4 md:py-4 py-2{% endif %}
          {% if link.child_active %}!underline{% endif %}
          {% if link.active %}!underline{% endif %}
          {% if section.settings.bar_layout_desktop == "center-below" %}
            px-4 mx-2
          {% else %}
            mr-2 pr-4
          {% endif %}">
          {{ link.title }}
          {% if link.levels >= 1 %}
            {% render 'component__icon', icon: 'chevron-down', class: 'ml-1', size: '16' %}
          {% endif %}
        </a>

        {% if link.levels >= 1 %}
          <div class="header__dropdown animation-300 absolute left-0 block w-full h-0 overflow-hidden normal-case opacity-0">
            <div class="border--t-width border--b-width
              {{ section.settings.dropdown_color_scheme }}
              {{ section.settings.dropdown_color_border }}">

              <div class="{{ settings.layout_horizontal }}">
                <div class="flex justify-between overflow-hidden 
                  {{ section.settings.dropdown_color_border }}">
               {%- comment -%}slider{%- endcomment -%}
                  {% assign block_count = 0 %}
                  {% for block in section.blocks %}
                    {% assign link_handle = link.handle | handle %}
                    {% assign menu_handle = block.settings.menu | handle %}
                    {% if link_handle == menu_handle %}
                      {% assign block_count = block_count | plus: 1 %}
                    {% endif %}
                  {% endfor %}
                  <div class="flex flex-wrap flex-grow ">
                    {% for child_link in link.links %}
                      <div class="py-8 pr-12">
                        <a href="{{ child_link.url }}" class="type__body--bold block mb-4 no-underline">
                          {{ child_link.title }}
                        </a>
                        {% for grandchild_link in child_link.links %}
                          <a href="{{ grandchild_link.url }}" class="type__body block mb-2 no-underline">
                            {{ grandchild_link.title }}
                          </a>
                        {% endfor %}
                        
                         {% assign index = forloop.index %}
                         {% for block in section.blocks %}
                          {%- case block.type -%}
                            {% when 'Image' %}
                              {% assign link_handle = link.handle | handle %}
                        {% assign menu_handle = block.settings.menu | handle %}
                        {% if link_handle == menu_handle %}
                              {% if block.settings.layout == 'bottom' %}
                                <div class="block-image-bottom">
                                {% for i in (1..8) %}
                                  {% if index == i %}
                                  {% capture itemsImage %}image{{- i -}}{% endcapture %}
                                  {% capture itemsUrl %}url{{- i -}}{% endcapture %}
                                  <a href="{{ block.settings[itemsUrl] }}"><img src="{{ block.settings[itemsImage] | img_url:'master' }}" /></a>
                                    {% endif %}
                                {% endfor %}
                                 </div>
                              {% endif %}
                              {% endif %}
                           {% endcase %}
                          {% endfor %}  
                      </div>
                    {% endfor %}
                  </div>

                  {% if block_count > 0 %}
                    {%- capture slides -%}
                      {% assign block_count = 0 %}
                      {% for block in section.blocks %}
                        
                      {%- case block.type -%}
                      {%- when 'Content' -%}
                        {% assign link_handle = link.handle | handle %}
                        {% assign menu_handle = block.settings.menu | handle %}
                        {% if link_handle == menu_handle %}
                          {% assign block_count = block_count | plus: 1 %}
                          <div class="md:snap-center flex justify-center flex-none max-w-[256px] whitespace-normal" 
                            x-intersect:enter.full="index = {{ block_count | minus: 1 }};"
                            x-bind:data-slide="{{ block_count | minus: 1 }}">
                            {% render 'component__card', 
                              url: block.settings.url,
                              image: block.settings.image,
                              title: block.settings.heading, 
                              content: block.settings.content %}
                          </div>
                        {% endif %}
                      
                       {% endcase %}
                      {% endfor %}  
                      
                      {%- capture outer_class -%}
                        py-8 max-w-[50%]
                      {%- endcapture -%}
                    {%- endcapture -%}
                    
                   {%- capture slides1 -%}
                      {% assign block_count = 0 %}
                      {% for block in section.blocks %}
                         {% assign link_handle = link.handle | handle %}
                        {% assign menu_handle = block.settings.menu | handle %}
                        {% if link_handle == menu_handle %}
                      {%- case block.type -%}
                        {% when 'Image' %}
                          {% if block.settings.layout == 'left' %}
                             <div class="block-image-menu">
                               {% if block.settings.title != blank %}
                            <h3>{{ block.settings.title }}</h3>
                               {% endif %}
                            <div class="block-image-left">
                            {% for i in (1..8) %}
                              {% capture itemsImage %}image{{- i -}}{% endcapture %}
                              {% capture itemsUrl %}url{{- i -}}{% endcapture %}
                              {% if block.settings[itemsImage] != blank %}
                              <a href="{{ block.settings[itemsUrl] }}"><img src="{{ block.settings[itemsImage] | img_url:'master' }}" /></a>
                                {% endif %}
                            {% endfor %}
                            </div>
                             </div>
                          {% endif %}
                       {% endcase %}
                        {% endif %}
                      {% endfor %}  
                      
                      {%- capture outer_class -%}
                    
                      {%- endcapture -%}
                    {%- endcapture -%}
                    {{ slides1 }}
                    {%- if block_count > 2 -%}
                      {%- capture show_arrows -%}true{%- endcapture -%}
                    {%- else -%}
                      {%- capture show_arrows -%}{%- endcapture -%}
                    {%- endif -%}

                    {%- if block_count > 0 -%}
                      {% assign block_count = block_count | minus: 1 %}
                    {%- endif -%}

                    {% render 'component__slider',
                      show_arrows: show_arrows,
                      outer_class: outer_class,
                      container_class: '',
                      slider_size: block_count,
                      slides: slides %}
                      
                  {% endif %}

                </div>
              </div>
              
            </div>
          </div>
        {% endif %}
        
      </div>
    {%- endfor -%}

  </div>
{% endcapture %}

{% capture desktop_search %}
  {% if section.settings.enable_search %}
    {% render 'header__search-desktop' %} 
  {% endif %}
{% endcapture %}

{% capture desktop_actions %}
  <div class="flex items-center justify-end flex-shrink pl-2 text-right">

    {% if section.settings.enable_search %}
      {% if section.settings.bar_layout_desktop == "left-inline" %}
        {% if section.settings.bar_button_search_type == "text" %}
          <a
            class="hover:underline inline-flex pl-4 ml-2 no-underline
            {% if settings.type_header == 'body' %}type__body{% endif %}
            {% if settings.type_header == 'heading' %}type__heading{% endif %}
            {% if settings.type_header == 'navigation' %}type__nav{% endif %}
            {{ settings.type_header_capitilization }}"
            href="{{ routes.search_url }}"
            rel="nofollow"
            @click.prevent="search_visible = true; search_active = true; $nextTick(() => { document.querySelector('#search-desktop').focus(); });">
            {{ 'general.search.search' | t }}
          </a>
        {% else %}
          <a
            class="type__nav hover:underline inline-flex pl-4 ml-2 no-underline"
            href="{{ routes.search_url }}"
            rel="nofollow"
            title="{{ 'general.search.search' | t }}"
            @click.prevent="search_visible = true; search_active = true; $nextTick(() => { document.querySelector('#search-desktop').focus(); });">
            {% render 'component__icon', icon: 'search', size: '24', class: '' %} 
          </a>
        {% endif %}
      {% endif %}
    {% endif %}

    {%- if shop.customer_accounts_enabled -%}
     {% unless section.settings.bar_button_login_type == "none" %}
      {% if section.settings.bar_button_login_type == "text" %}
        <a
          class="hover:underline inline-flex pl-4 ml-2 no-underline
          {% if settings.type_header == 'body' %}type__body{% endif %}
          {% if settings.type_header == 'heading' %}type__heading{% endif %}
          {% if settings.type_header == 'navigation' %}type__nav{% endif %}
          {{ settings.type_header_capitilization }}"
          href="{{ routes.account_url }}"
          rel="nofollow">
          {% if customer %}
            {{ 'general.navigation.account' | t }}
          {% else %}
            {{ 'customer.account.log_in' | t }}
          {% endif %}

        </a>
      {% else %}
        <a
          class="type__nav hover:underline inline-flex pl-4 ml-2 no-underline"
          href="{{ routes.account_url }}"
          rel="nofollow"
          title=" {% if customer %}{{ 'general.navigation.account' | t }}{% else %}{{ 'customer.account.log_in' | t }}{% endif %}">
          {% render 'component__icon', icon: 'account', size: '24', class: '' %} 
        </a>
      {% endif %}
     {% endunless %}
    {% endif %}

    
    {% if section.settings.bar_button_cart_type == "text" %}
      <a
        class="inline-flex pl-4 ml-2 no-underline items-center
        {% if settings.type_header == 'body' %}type__body{% endif %}
        {% if settings.type_header == 'heading' %}type__heading{% endif %}
        {% if settings.type_header == 'navigation' %}type__nav{% endif %}
        {{ settings.type_header_capitilization }}"
        {% if settings.cart_icon_behavior == "open_cart_drawer" %} @click.prevent="cart_drawer = true; if(enable_audio) { playSound(click_audio)}"{% endif %}
        href="{{ routes.cart_url }}"
        rel="nofollow"
        >
        {{ 'cart.general.title' | t }} 
        <span class="ml-1
        {% if settings.type_header == 'body' %}type__body{% endif %}
        {% if settings.type_header == 'heading' %}type__heading{% endif %}
        {% if settings.type_header == 'navigation' %}type__nav{% endif %}
        {{ settings.type_header_capitilization }}" 
          x-show="cart.item_count > 0" 
          x-cloak>(<span class="
          {% if settings.type_header == 'body' %}type__body{% endif %}
          {% if settings.type_header == 'heading' %}type__heading{% endif %}
          {% if settings.type_header == 'navigation' %}type__nav{% endif %}
          {{ settings.type_header_capitilization }}" x-text="cart.item_count">{{ cart.item_count }}</span>)</span>
      </a>
    {% else %}
      <a
        class="type__nav relative inline-flex items-center pl-4 ml-2 !no-underline"
        href="{{ routes.cart_url }}"
        rel="nofollow"
        title="{{ 'cart.general.title' | t }}"
        {% if settings.cart_icon_behavior == "open_cart_drawer" %}@click.prevent="cart_drawer = true; if(enable_audio) { playSound(click_audio)}"{% endif %}>
        {% render 'component__icon', icon: 'cart', size: '24', class: '' %} 
        <span 
          class="type__nav type--smaller color__bg-primary color__primary ml-1 px-2 py-0.5 border__button--radius"
          x-show="cart.item_count > 0" 
          x-cloak>
          <span class="type__nav evm_wiser_count" x-text="cart.item_count">{{ cart.item_count }}</span>
        </span>
      </a>
    {% endif %}

  </div>
{% endcapture %}

{% capture desktop_actions--minor %}
  <div class="basis-0 flex items-center justify-start flex-grow flex-shrink pr-2
    {% if section.settings.bar_layout_desktop == "center-below" %}grow-0{% endif %}">

    {% if section.settings.enable_search %}
      {% unless section.settings.bar_layout_desktop == "left-below" %}
        {% if section.settings.bar_button_search_type == "text" %}
          <a
            class="hover:underline inline-flex pl-0 ml-0 no-underline
            {% if settings.type_header == 'body' %}type__body{% endif %}
            {% if settings.type_header == 'heading' %}type__heading{% endif %}
            {% if settings.type_header == 'navigation' %}type__nav{% endif %}
            {{ settings.type_header_capitilization }}"
            href="{{ routes.search_url }}"
            rel="nofollow"
            @click.prevent="search_visible = true; search_active = true; $nextTick(() => { document.querySelector('#search-desktop').focus(); });">
            {{ 'general.search.search' | t }}
          </a>
        {% else %}
          <a
            class="type__nav hover:underline inline-flex pl-0 ml-0 no-underline"
            href="{{ routes.search_url }}"
            rel="nofollow"
            title="{{ 'general.search.search' | t }}"
            @click.prevent="search_visible = true; search_active = true; $nextTick(() => { document.querySelector('#search-desktop').focus(); });">
            {% render 'component__icon', icon: 'search', size: '24', class: '' %} 
          </a>
        {% endif %}
      {% endunless %}
    {% endif %}
    
  </div>
{% endcapture %}

{% capture desktop_search %}
  {% if section.settings.enable_search %}
    {% render 'header__search-desktop' %} 
  {% endif %}
{% endcapture %}

<nav
  class="
    border--b-width
    {{ section.settings.visibility }}
    {{ section.settings.bar_color_scheme }}
    {{ section.settings.bar_color_border }}
  "
  x-data="
    {
      search_visible: {% if section.settings.bar_layout_desktop == "left-below" %}true{% else %}false{% endif %},
      search_active: false
    }
  "
>
  <div class="relative">
    {%- comment -%}mobile{%- endcomment -%}
    <div
      class="
        md:hidden flex flex-wrap items-center py-2
        {{ settings.layout_horizontal }}
      "
    >
      {%- comment -%}mobile bar{%- endcomment -%}
      <div class="basis-0 flex items-center justify-start flex-grow flex-shrink">
        {% if section.settings.bar_button_menu_type == 'text' %}
          <button
            class="
              no-js--hide inline-flex py-2 pr-4 mr-2 no-underline
              {% if settings.type_header == 'body' %}type__body{% endif %}
              {% if settings.type_header == 'heading' %}type__heading{% endif %}
              {% if settings.type_header == 'navigation' %}type__nav{% endif %}
              {{ settings.type_header_capitilization }}
            "
            
            @click="menu_drawer = true; if(enable_audio) { playSound(click_audio) }"
          >
            {{ 'general.navigation.menu' | t }}
          </button>
        {% else %}
          <button
            @click="menu_drawer = true; if(enable_audio) { playSound(click_audio) }"
            class="type__nav no-js--hide inline-flex py-2 pr-4 mr-2 no-underline"
            title="{{ 'general.navigation.menu' | t }}"
          >
            {% render 'component__icon', icon: 'menu', size: '24', class: '' %}
          </button>
        {% endif %}

        <noscript>
          {% if section.settings.bar_button_menu_type == 'text' %}
            <button
              class="type__nav peer inline-flex py-2 pr-4 mr-2 no-underline"
              @click="menu_drawer = true; if(enable_audio) { playSound(click_audio) }"
            >
              {{ 'general.navigation.menu' | t }}
            </button>
          {% else %}
            <button
              @click="menu_drawer = true; if(enable_audio) { playSound(click_audio) }"
              class="type__nav peer inline-flex py-2 pr-4 mr-2 no-underline"
              title="{{ 'general.navigation.menu' | t }}"
            >
              {% render 'component__icon',
                icon: 'menu',
                size: '24',
                class: ''
              %}
            </button>
          {% endif %}

          <div class="peer-focus:block hover:block focus-within:block hidden bottom-0 fixed color__bg-body w-full left-0 right-0 -z-10 max-h-[70vh] mt-28 border--t-width color__border-divider-1 overflow-scroll">
            {%- for link in section.settings.dropdown_links.links -%}
              <a
                class="border--b-width color__border-divider-1 block w-full p-4 no-underline"
                href="{{ link.url }}"
              >
                {{- link.title -}}
              </a>
              {% for child_link in link.links %}
                <a
                  class="border--b-width color__border-divider-1 block w-full p-4 !pl-8 no-underline"
                  href="{{ child_link.url }}"
                >
                  {{- child_link.title -}}
                </a>
                {% for grandchild_link in child_link.links %}
                  <a
                    class="border--b-width color__border-divider-1 block w-full p-4 !pl-12 no-underline"
                    href="{{ grandchild_link.url }}"
                  >
                    {{- grandchild_link.title -}}
                  </a>
                {% endfor %}
              {% endfor %}
            {%- endfor -%}
          </div>
        </noscript>
      </div>
      <div class="flex items-center max-w-[45%] sm:max-w-[70%]">
        {% capture logo_size_light %}
          height: {{ section.settings.logo_mobile_height }}px;
          width: {{ section.settings.logo_mobile_height | times: section.settings.logo_light.aspect_ratio }}px;
        {% endcapture %}

          {% comment %} if logo is there {% endcomment %}
        {% if section.settings.logo_light %}
          <a
            class="!inline-block relative flex items-center hidden"
            style="{{ logo_size_light }}"
            href="{{ routes.root_url }}"
            rel="nofollow"
            title="{{ shop.name }}"
          >
            {{
              section.settings.logo_light
              | image_url: width: 400
              | image_tag:
                class: 'w-full h-full !bg-transparent object-contain',
                alt: section.settings.logo_light.alt
            }}
          </a>
          {% comment %} if not, we serve text {% endcomment %}
        {% else %}
          <a
            class="!inline-block relative flex items-center no-underline whitespace-nowrap"
            href="{{ routes.root_url }}"
            rel="nofollow"
            title="{{ shop.name }}"
          >
            {{ shop.name }}
          </a>
        {% endif %}
      </div>
      <div class="basis-0 flex items-center justify-end flex-grow flex-shrink text-right">
        {% if section.settings.bar_button_cart_type == 'text' %}
          <a
            class="
              inline-flex py-2 pl-4 ml-2 no-underline
              {% if settings.type_header == 'body' %}type__body{% endif %}
              {% if settings.type_header == 'heading' %}type__heading{% endif %}
              {% if settings.type_header == 'navigation' %}type__nav{% endif %}
              {{ settings.type_header_capitilization }}
            "
            href="{{ routes.cart_url }}"
            rel="nofollow"
            {% if settings.cart_icon_behavior == "open_cart_drawer" %}@click.prevent="cart_drawer = true; if(enable_audio) { playSound(click_audio) }"{% endif %}
          >
            {{ 'cart.general.title' | t }}
            <span
              class="
                ml-1
                {% if settings.type_header == 'body' %}type__body{% endif %}
                {% if settings.type_header == 'heading' %}type__heading{% endif %}
                {% if settings.type_header == 'navigation' %}type__nav{% endif %}
                {{ settings.type_header_capitilization }}
              "
              x-show="cart.item_count > 0"
              x-cloak
              >(<span
                class="
                  {% if settings.type_header == 'body' %}type__body{% endif %}
                  {% if settings.type_header == 'heading' %}type__heading{% endif %}
                  {% if settings.type_header == 'navigation' %}type__nav{% endif %}
                  {{ settings.type_header_capitilization }}
                "
                x-text="cart.item_count"
              >
                {{- cart.item_count -}}</span
              >)
            </span>
          </a>
        {% else %}
          <a
            class="type__nav relative inline-flex items-center py-2 pl-4 ml-2 no-underline"
            href="{{ routes.cart_url }}"
            rel="nofollow"
            title="{{ 'cart.general.title' | t }}"
            {% if settings.cart_icon_behavior == "open_cart_drawer" %}@click.prevent="cart_drawer = true; if(enable_audio) { playSound(click_audio) }"{% endif %}
          >
            {% render 'component__icon', icon: 'cart', size: '24', class: '' %}
            <span
              class="type__nav type--smaller color__bg-primary color__primary ml-1 px-2 py-0.5 border__button--radius"
              x-show="cart.item_count > 0"
              x-cloak
            >
              <span class="type__nav" x-text="cart.item_count">
                {{- cart.item_count -}}
              </span>
            </span>
          </a>
        {% endif %}
      </div>

      {%- comment -%}mobile search{%- endcomment -%}
      {% if section.settings.enable_search %}
        {% render 'header__search-mobile' %}
      {% endif %}
    </div>

    {%- comment -%}desktop{%- endcomment -%}
    <div
      class="
        md:flex items-stretch justify-between hidden
        {{ settings.layout_horizontal }}
        {% if section.settings.bar_layout_desktop == "left-inline" %}
          flex-nowrap
        {% endif %}
        {% if section.settings.bar_layout_desktop == "left-below" %}
          flex-wrap
        {% endif %}
        {% if section.settings.bar_layout_desktop == "center-below" %}
          flex-wrap
        {% endif %}
      "
    >
      {% if section.settings.bar_layout_desktop == 'left-inline' %}
        {{ desktop_search }}
        {{ desktop_logo }}
        {{ desktop_navigation }}
        {{ desktop_actions }}
      {% endif %}

      {% if section.settings.bar_layout_desktop == 'left-below' %}
        {{ desktop_logo }}
        {{ desktop_search }}
        {{ desktop_actions }}
        {{ desktop_navigation }}
      {% endif %}

      {% if section.settings.bar_layout_desktop == 'center-below' %}
        {{ desktop_search }}
        {{ desktop_actions--minor }}
        {{ desktop_logo }}
        {{ desktop_actions }}
        {{ desktop_navigation }}
      {% endif %}
    </div>
  </div>
</nav>
<style>
  .block-image-menu{max-width:50%;width:100%;padding-right:5%;order:-1;padding-top:30px;padding-left:30px;}
  .block-image-left{display:flex;flex-wrap:wrap;}
  .block-image-left a{max-width:25%;padding:10px;}
  .block-image-bottom img{max-width:200px;}
  .block-image-left img{max-width:140px;}
  .block-image-menu h3{font-size:20px;}
</style>
{% schema %}
{
  "name": "t:sections.theme_header.name",
  "settings": [
    {
      "type": "header",
      "content": "t:sections.all.headers.header__15.content"
    },
    {
      "type": "link_list",
      "id": "dropdown_links",
      "label": "t:sections.theme_header.settings.dropdown_links.label",
      "default": "main-menu"
    },
    {
      "type": "link_list",
      "id": "search_links",
      "label": "t:sections.theme_header.settings.search_links.label",
      "info": "t:sections.theme_header.settings.search_links.info"
    },
    {
      "type": "header",
      "content": "t:sections.all.headers.header__16.content"
    },
    {
      "type": "image_picker",
      "id": "logo_light",
      "label": "t:sections.theme_header.settings.logo_light.label"
    },
    {
      "type": "number",
      "id": "logo_desktop_height",
      "label": "t:sections.theme_header.settings.logo_desktop_height.label",
      "default": 200
    },
    {
      "type": "number",
      "id": "logo_mobile_height",
      "label": "t:sections.theme_header.settings.logo_mobile_height.label",
      "default": 150
    },
    {
      "type": "header",
