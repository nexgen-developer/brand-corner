<!-- sections/product__main.liquid -->
{% comment %} 
  Loop through different blocks to create a product page.

  Globals:
    - settings.layout_horizontal: horizontaly margin
    - settings.thumbmails_color_scheme: set color of items

  Possible enhancements:
    - Add alternative layouts
    - Add support so upsell can fit within form

  Pro tips:
    - Multiple product templates can be created from the theme editor.
{% endcomment %}

{% assign fixedButtonEnabled = false %}
{% for block in section.blocks %}
  {% if block.type == "stickyadd" %}
    {% assign fixedButtonEnabled = true %}
  {% endif %}
{% endfor %}

<section class="z-10 relative
  {{ section.settings.color_scheme }}"
  style="
    padding-top:{{ section.settings.spacing_top }}px;
    padding-bottom:{{ section.settings.spacing_bottom }}px;"
  x-data="{ 
    fullscreen: false,
    stickyadd: false
  }"
  {% if fixedButtonEnabled %}
    x-intersect:enter="stickyadd = false; scroll_up_force = true;"
    x-intersect:leave="stickyadd = true; scroll_up_force = false;"
  {% endif %}
 >

  {%- capture script_product -%}
    {% render 'script__product', 
      gallery_style : section.settings.media_gallery_style %}
  {%- endcapture -%}
  
  <div class="!px-0 md:!px-4 flex flex-row flex-wrap
    {{ settings.layout_horizontal }}"
    x-data="{{ script_product }}"
    x-init="
      initForm();
      checkLineItems('{{ product.handle }}');"
    x-ref="formContainer">

    {% comment %} Media - left column {% endcomment %}
    <div class="top-0 md:sticky flex flex-col
      {{ section.settings.layout_gallery_size }}">

      {%- comment -%} Media {%- endcomment -%}
      {% if section.settings.media_gallery_style == 'grid' %}
        {% render 'product__gallery-grid' %}
      {% elsif section.settings.media_gallery_style == 'slider' %}
        {% render 'product__gallery-slider' %}
      {% else %}
        {% render 'product__gallery-thumbnail-slider' %} 
      {% endif %}

      {% comment %} Left column blocks {% endcomment %}
      <div class="md:block hidden" x-data="{ 
        button_loading: false 
      }">
        
          {% for block in section.blocks %}
            {%- case block.type -%}
              {%- when 'breadcrumb' -%}
                {% if block.settings.position == "left" %}
                  {% render 'product__breadcrumb', blocksettings: block.settings %}
                {% endif %}
              
              {%- when 'title' -%}
                {% if block.settings.position == "left" %}
                  {% render 'product__title', blocksettings: block.settings %}
                {% endif %}

              {%- when 'price' -%}
                {% if block.settings.position == "left" %}
                  <div class="px-4 color__border-divider-1
                      {{ block.settings.color_scheme }} 
                      {{ block.settings.style_border }}"
                    style="
                      padding-top:{{ block.settings.spacing_top }}px;
                      padding-bottom:{{ block.settings.spacing_bottom }}px;">
                    <div class="relative flex items-end justify-between">
                      {% render 'product__price', blocksettings: block.settings %}
                    </div>
                    <div class="type--small mt-1">
                      {{ form | payment_terms }}
                    </div>
                  </div>
                {% endif %}

              {%- when 'custom' -%}
                {% if block.settings.position == "left" %}
                  <div class='px-4 color__border-divider-1
                    {{ block.settings.color_scheme }} 
                    {{ block.settings.style_border }}'
                    style="
                      padding-top:{{ block.settings.spacing_top }}px;
                      padding-bottom:{{ block.settings.spacing_bottom }}px;">
                    {% render 'product__custom-field', blocksettings: block.settings %}
                  </div>
                {% endif %}

              {%- when 'text' -%}
                {% if block.settings.position == "left" %}
                  {% render 'product__text', blocksettings: block.settings %}
                {% endif %}

              {%- when 'description' -%}
                {% if block.settings.position == "left" %}
                  <div style="
                    padding-top:{{ block.settings.spacing_top }}px;
                    padding-bottom:{{ block.settings.spacing_bottom }}px;">
                    {% render 'product__description', blocksettings: block.settings %}
                  </div>
                {% endif %}

              {%- when 'accordion' -%}
                {% if block.settings.position == "left" %}
                  {% render 'product__accordion', blocksettings: block.settings %}
                {% endif %}

              {%- when 'inventory' -%}
                {% if block.settings.position == "left" %}
                  {% render 'product__inventory', blocksettings: block.settings %}
                {% endif %}

              {%- when 'pickup' -%}
                {% if block.settings.position == "left" %}
                  {% render 'product__pickup', blocksettings: block.settings %}  
                {% endif %}

              {%- when 'linked_products' -%}
                {% if block.settings.position == "left" %}
                  <div class="color__border-divider-1
                    {{ block.settings.color_scheme }}
                    {{ block.settings.style_border }}"
                    style="
                    padding-top:{{ block.settings.spacing_top }}px;
                    padding-bottom:{{ block.settings.spacing_bottom }}px;">
                    {% render 'product__linked-products', blocksettings: block.settings %}
                  </div>
                {% endif %}


              {%- when 'rating' -%}
                {% if block.settings.position == "left" %}
                  {% render 'product__rating', blocksettings: block.settings %}
                {% endif %}

              {% when 'liquid' %}
                {% if block.settings.position == "left" %}
                  {% render 'product__liquid', blocksettings: block.settings %}
                {% endif %}

            {%- endcase -%}
          {% endfor %}
        {% for block in section.blocks %}
          {%- case block.type -%}
            {%- when 'upsell' -%}
              {% if block.settings.position == "left" %}
                {% render 'product__upsell', blocksettings: block.settings %}
              {% endif %}
          {%- endcase -%}
        {% endfor %}
      </div>

    </div>
    
    {%- comment -%} Details - right column {%- endcomment -%}
    {% case section.settings.layout_gallery_size %}
      {% when 'w-full md:w-1/3' %}
        {% assign layout_detail_size = 'w-full md:w-2/3' %}
      {% when 'w-full md:w-1/2' %}
        {% assign layout_detail_size = 'w-full md:w-1/2' %}
      {% when 'w-full md:w-2/3' %}
        {% assign layout_detail_size = 'w-full md:w-1/3' %}
      {% when 'w-full md:w-3/4' %}
        {% assign layout_detail_size = 'w-full md:w-1/4' %}
    {% endcase %}
    <div class="md:border--l-width md:border--r-width border--t-width md:border-t-0
      {{ section.settings.color_border }}
      {{ layout_detail_size }}">
      <div :class="{ 
        '-z-[1]' : reduce_product_zindex
      }" class="sticky top-0 z-10" 
        x-data="{ 
          button_loading: false 
        }">

        {% form "product", product, data-productid: product.id, x-ref: 'productForm' %}
          
          {% for block in section.blocks %}
            {%- case block.type -%}
              
              {%- when 'breadcrumb' -%}
                {% if block.settings.position == "right" %}
                  {% render 'product__breadcrumb', blocksettings: block.settings %}
                {% endif %}
              
              {%- when 'title' -%}
                {% if block.settings.position == "right" %}
                  {% render 'product__title', blocksettings: block.settings %}
                {% endif %}
              {%- when 'price' -%}
                {% if block.settings.position == "right" %}
                  <div class="px-4 color__border-divider-1
                      {{ block.settings.color_scheme }} 
                      {{ block.settings.style_border }}"
                    style="
                      padding-top:{{ block.settings.spacing_top }}px;
                      padding-bottom:{{ block.settings.spacing_bottom }}px;">
                    <div class="relative flex items-end justify-between">
                      {% render 'product__price', blocksettings: block.settings %}
                    </div>
                    <div class="type--small mt-1">
                      {{ form | payment_terms }}
                    </div>
                  </div>
                {% endif %}

             <!-- Start of product colour linking -->
             <div id="webyze-product-colors-{{ product.id }}"></div>
             <!-- End of product colour linking -->

              {%- when 'custom' -%}
                {% if block.settings.position == "right" %}
                  <div class='px-4 color__border-divider-1
                    {{ block.settings.color_scheme }} 
                    {{ block.settings.style_border }}'
                    style="
                      padding-top:{{ block.settings.spacing_top }}px;
                      padding-bottom:{{ block.settings.spacing_bottom }}px;">
                    {% render 'product__custom-field', blocksettings: block.settings %}
                  </div>
                {% endif %}
               
              {%- when 'text' -%}
                {% if block.settings.position == "right" %}
                  {% render 'product__text', blocksettings: block.settings %}
                {% endif %}

              {%- when 'description' -%}
                {% if block.settings.position == "right" %}
                  <div style="
                    padding-top:{{ block.settings.spacing_top }}px;
                    padding-bottom:{{ block.settings.spacing_bottom }}px;">
                    {% render 'product__description', blocksettings: block.settings %}
                  </div>
                {% endif %}

              {%- when 'accordion' -%}
                {% if block.settings.position == "right" %}
                  {% render 'product__accordion', blocksettings: block.settings %}
                {% endif %}

              {%- when 'popup' -%}
                  {% render 'product__popup', blocksettings: block.settings %}

              {%- when 'inventory' -%}
                {% if block.settings.position == "right" %}
                  {% render 'product__inventory', blocksettings: block.settings %}
                {% endif %}
                
              {%- when 'pickup' -%}
                {% if block.settings.position == "right" %}
                  {% render 'product__pickup', blocksettings: block.settings %}  
                {% endif %}  
              {%- when 'options' -%}
                  <div class="options color__border-divider-1
                    {{ block.settings.color_scheme }}
                    {{ block.settings.style_border }}"
                    style="
                      padding-top:{% if product.selected_or_first_available_selling_plan_allocation or product.has_only_default_variant == false %}{{ block.settings.spacing_top }}px;{% else %}0;{% endif %}
                      padding-bottom:{% if product.selected_or_first_available_selling_plan_allocation or product.has_only_default_variant == false %}{{ block.settings.spacing_bottom }}px;{% else %}0;{% endif %}">
                    {% render 'product__options', 
                      blocksettings: block.settings %}
                  </div>
              
              {%- when 'linked_products' -%}
                {% if block.settings.position == "right" %}
                  <div class="color__border-divider-1
                    {{ block.settings.color_scheme }}
                    {{ block.settings.style_border }}"
                    style="
                    padding-top:{{ block.settings.spacing_top }}px;
                    padding-bottom:{{ block.settings.spacing_bottom }}px;">
                    {% render 'product__linked-products', blocksettings: block.settings %}
                  </div>
                {% endif %}
                
              {%- when 'add' -%}
                <div class="color__border-divider-1
                  {{ block.settings.color_scheme }}
                  {{ block.settings.style_border }}"
                  style="
                    padding-top:{{ block.settings.spacing_top }}px;
                    padding-bottom:{{ block.settings.spacing_bottom }}px;">
                    
                  {% render 'product__add', 
                    blocksettings: block.settings %}
                  {% if block.settings.enable_dynamic_checkout %}
                    <div class="px-4 mt-4">
                      {{ form | payment_button }}
                    </div>
                  {% endif %}
                  {%- unless block.settings.content == blank -%}
                    <div class="w-full px-4 mt-4">
                      <div class="type--small m-0--clear color__bg-overlay-1 border--radius p-2 text-center">
                        {{ block.settings.content }}
                      </div>
                    </div>
                  {% endunless %}
                </div>
  
              {%- when 'stickyadd' -%}
                {% render 'product__stickyadd', blocksettings: block.settings %}

              {%- when 'rating' -%}
                {% if block.settings.position == "right" %}
                  {% render 'product__rating', blocksettings: block.settings %}
                {% endif %}
    
              {% when 'liquid' %}
                {% if block.settings.position == "right" %}
                  {% render 'product__liquid', blocksettings: block.settings %}
                {% endif %}
    
              {% when '@app' %}
                <div class="w-full px-4 mt-4">
                  {% render block %}
                 </div>
            {%- endcase -%}
          {% endfor %}
          <div class="md:hidden">
            {% for block in section.blocks %}
              {%- case block.type -%}
                {%- when 'breadcrumb' -%}
                  {% if block.settings.position == "left" %}
                    {% render 'product__breadcrumb', blocksettings: block.settings %}
                  {% endif %}
                
                {%- when 'title' -%}
                  {% if block.settings.position == "left" %}
                    {% render 'product__title', blocksettings: block.settings %}
                  {% endif %}

                {%- when 'price' -%}
                  {% if block.settings.position == "left" %}
                    <div class="px-4 color__border-divider-1
                        {{ block.settings.color_scheme }} 
                        {{ block.settings.style_border }}"
                      style="
                        padding-top:{{ block.settings.spacing_top }}px;
                        padding-bottom:{{ block.settings.spacing_bottom }}px;">
                      <div class="relative flex items-end justify-between">
                        {% render 'product__price', blocksettings: block.settings %}
                      </div>
                      <div class="type--small mt-1">
                        {{ form | payment_terms }}
                      </div>
                    </div>
                  {% endif %}

                {%- when 'custom' -%}
                  {% if block.settings.position == "left" %}
                    <div class='px-4 color__border-divider-1
                      {{ block.settings.color_scheme }} 
                      {{ block.settings.style_border }}'
                      style="
                        padding-top:{{ block.settings.spacing_top }}px;
                        padding-bottom:{{ block.settings.spacing_bottom }}px;">
                      {% render 'product__custom-field', blocksettings: block.settings %}
                    </div>
                  {% endif %}

                {%- when 'text' -%}
                  {% if block.settings.position == "left" %}
                    {% render 'product__text', blocksettings: block.settings %}
                  {% endif %}

                {%- when 'description' -%}
                  {% if block.settings.position == "left" %}
                    <div style="
                      padding-top:{{ block.settings.spacing_top }}px;
                      padding-bottom:{{ block.settings.spacing_bottom }}px;">
                      {% render 'product__description', blocksettings: block.settings %}
                    </div>
                  {% endif %}

                 {% comment %} {%- when 'accordion' -%}
                  {% if block.settings.position == "left" %}
                  {% render 'product__accordion', blocksettings: block.settings %}
                  {% endif %}{% endcomment %}
                
                {%- when 'inventory' -%}
                  {% if block.settings.position == "left" %}
                    {% render 'product__inventory', blocksettings: block.settings %}
                  {% endif %}

                {%- when 'pickup' -%}
                  {% if block.settings.position == "left" %}
                    {% render 'product__pickup', blocksettings: block.settings %}  
                  {% endif %}

                  {%- when 'options' -%}
                    {% if block.settings.position == "left" %}
                      <div class="options color__border-divider-1
                        {{ block.settings.color_scheme }}
                        {{ block.settings.style_border }}"
                        style="
                          padding-top:{% if product.selected_or_first_available_selling_plan_allocation or product.has_only_default_variant == false %}{{ block.settings.spacing_top }}px;{% else %}0;{% endif %}
                          padding-bottom:{% if product.selected_or_first_available_selling_plan_allocation or product.has_only_default_variant == false %}{{ block.settings.spacing_bottom }}px;{% else %}0;{% endif %}">
                        {% render 'product__options', 
                          blocksettings: block.settings %}
                      </div>
                    {% endif %}
          
                    {%- when 'add' -%}
                      {% if block.settings.position == "left" %}
                        <div class="color__border-divider-1
                          {{ block.settings.color_scheme }}
                          {{ block.settings.style_border }}"
                          style="
                            padding-top:{{ block.settings.spacing_top }}px;
                            padding-bottom:{{ block.settings.spacing_bottom }}px;">
                            
                          {% render 'product__add', 
                            blocksettings: block.settings %}
                          {% if block.settings.enable_dynamic_checkout %}
                            <div class="px-4 mt-4">
                              {{ form | payment_button }}
                            </div>
                          {% endif %}
                          {%- unless block.settings.content == blank -%}
                            <div class="w-full px-4 mt-4">
                              <div class="type--small m-0--clear color__bg-overlay-1 border--radius p-2 text-center">
                                {{ block.settings.content }}
                              </div>
                            </div>
                          {% endunless %}
                        </div>
                      {% endif %}

                {%- when 'linked_products' -%}
                  {% if block.settings.position == "left" %}
                    <div class="color__border-divider-1
                      {{ block.settings.color_scheme }}
                      {{ block.settings.style_border }}"
                      style="
                      padding-top:{{ block.settings.spacing_top }}px;
                      padding-bottom:{{ block.settings.spacing_bottom }}px;">
                      {% render 'product__linked-products', blocksettings: block.settings %}
                    </div>
                  {% endif %}

                

                {%- when 'rating' -%}
                  {% if block.settings.position == "left" %}
                    {% render 'product__rating', blocksettings: block.settings %}
                  {% endif %}

                {% when 'liquid' %}
                  {% if block.settings.position == "left" %}
                    {% render 'product__liquid', blocksettings: block.settings %}
                  {% endif %}

              {%- endcase -%}
            {% endfor %}
          </div>
        {% endform %}
        {% for block in section.blocks %}
          {%- case block.type -%}
            {%- when 'upsell' -%}
              {% if block.settings.position == "right" %}
                {% render 'product__upsell', blocksettings: block.settings %}
              {% endif %}
          {%- endcase -%}
        {% endfor %}
        <div class="md:hidden">
          {% for block in section.blocks %}
            {%- case block.type -%}
              {%- when 'upsell' -%}
                {% if block.settings.position == "left" %}
                  {% render 'product__upsell', blocksettings: block.settings %}
                {% endif %}
            {%- endcase -%}
          {% endfor %}
        </div>
      </div>
    </div>

  </div>

  {% if section.settings.enable_zoom %}
    {% render 'product__gallery-fullscreen' %}
  {% endif %}

</section>

{% render 'script__recent-view' %}

{% schema %}
{
  "name": "t:sections.product_main.name",
  "settings": [
    {
      "type": "checkbox",
      "id": "enable_default_variant",
      "label": "t:sections.product_main.settings.enable_default_variant.label",
      "info": "t:sections.product_main.settings.enable_default_variant.info",
      "default": true
    },
    {
        "type": "checkbox",
        "id": "enable_default_selling_plan_widget",
        "label": "t:settings_schema.product.settings.enable_default_selling_plan_widget.label",
        "info": "t:settings_schema.product.settings.enable_default_selling_plan_widget.info",
        "default": true
    },
    {
      "type": "header",
      "content": "t:sections.all.headers.header__22.content"
    },
    {
      "type":"range",
      "id":"spacing_top",
      "min":0,
      "max":300,
      "step":20,
      "unit":"px",
      "label":"t:sections.all.layout.settings.spacing.label_top",
      "default":0
    },
    {
      "type":"range",
      "id":"spacing_bottom",
      "min":0,
      "max":300,
      "step":20,
      "unit":"px",
      "label":"t:sections.all.layout.settings.spacing.label_bottom",
      "default":0
    },
    {
      "type": "header",
      "content": "t:sections.all.headers.header__10.content"
    },
    {
      "type": "select",
      "id": "media_gallery_style",
      "label": "t:sections.product_main.settings.media_gallery_style.label",
      "options": [
        {
          "value": "grid",
          "label": "t:sections.product_main.settings.media_gallery_style.options__1.label"
        },
        {
          "value": "slider",
          "label": "t:sections.product_main.settings.media_gallery_style.options__2.label"
        },
        {
          "value": "thumbnail-slider",
          "label": "t:sections.product_main.settings.media_gallery_style.options__3.label"
        }
      ],
      "default": "grid"
    },
    {
      "type":"select",
      "id":"image_border",
      "label":"t:sections.all.colors.settings.color_border.label",
      "options":[
        {
          "value":"color__border-divider-1",
          "label":"t:sections.all.colors.settings.color_border.options__1.label"
        },
        {
          "value":"color__border-selected-1",
          "label":"t:sections.all.colors.settings.color_border.options__2.label"
        },
        {
          "value":"!color__border-transparent",
          "label":"t:sections.all.colors.settings.color_border.options__3.label"
        }
      ],
      "default":"!color__border-transparent"
    },
    {
      "type": "checkbox",
      "id": "enable_zoom",
      "label": "t:sections.product_main.settings.enable_zoom.label",
      "info": "t:sections.product_main.settings.enable_zoom.info",
      "default": true
    },
    {
      "type": "checkbox",
      "id": "enable_alt",
      "label": "t:sections.product_main.settings.enable_alt.label",
      "info": "t:sections.product_main.settings.enable_alt.info",
      "default": false
    },
    {
      "type": "checkbox",
      "id": "enable_multiple_variant_images",
      "label": "t:sections.product_main.settings.enable_multiple_variant_images.label",
      "info": "t:sections.product_main.settings.enable_multiple_variant_images.info",
      "default": false
    },
    {
      "type": "header",
      "content": "t:sections.all.headers.header__21.content"
    },
    {
      "type": "select",
      "id": "media_color_scheme",
      "label": "t:sections.all.colors.settings.color_scheme.label",
      "options": [
        {
          "value": "color__bg-body color__text",
          "label": "t:sections.all.colors.settings.color_scheme.options__1.label"
        },
        {
          "value": "color__bg-neutral color__text",
          "label": "t:sections.all.colors.settings.color_scheme.options__2.label"
        },
        {
          "value": "color__bg-overlay-1 color__text",
          "label": "t:sections.all.colors.settings.color_scheme.options__3.label"
        },
        {
          "value": "color__bg-overlay-2 color__text",
          "label": "t:sections.all.colors.settings.color_scheme.options__4.label"
        },
        {
          "value": "color__bg-overlay-3 color__text",
          "label": "t:sections.all.colors.settings.color_scheme.options__5.label"
        },
        {
          "value": "color__bg-primary color__primary",
          "label": "t:sections.all.colors.settings.color_scheme.options__6.label"
        },
        {
          "value": "color__bg-secondary color__secondary",
          "label": "t:sections.all.colors.settings.color_scheme.options__7.label"
        },
        {
          "value":"color__bg-tertiary color__tertiary",
          "label":"t:sections.all.colors.settings.color_scheme.options__8.label"
        }
      ],
      "default": "color__bg-overlay-1 color__text"
    },
    {
      "type": "select",
      "id": "media_ratio",
      "label": "t:sections.product_main.settings.media_ratio.label",
      "options": [
        {
          "value": "aspect-[2/3] aspect-w-2 aspect-h-3",
          "label": "t:sections.product_main.settings.media_ratio.options__1.label"
        },
        {
          "value": "aspect-[1/1] aspect-w-1 aspect-h-1",
          "label": "t:sections.product_main.settings.media_ratio.options__2.label"
        }
      ],
      "default": "aspect-[2/3] aspect-w-2 aspect-h-3"
    },
    {
      "type": "select",
      "id": "media_object-sizing",
      "label": "t:sections.product_main.settings.media_object-sizing.label",
      "options": [
        {
          "value": "contain",
          "label": "t:sections.product_main.settings.media_object-sizing.options__1.label"
        },
        {
          "value": "cover",
          "label": "t:sections.product_main.settings.media_object-sizing.options__2.label"
        }
      ],
      "default": "cover"
    },
    {
      "type": "checkbox",
      "id": "enable_ratio",
      "label": "t:sections.product_main.settings.enable_ratio.label",
      "info": "t:sections.product_main.settings.enable_ratio.info",
      "default": false
    },
    {
      "type": "checkbox",
      "id": "show_product_badges",
      "label": "t:sections.product_main.settings.show_product_badges.label",
      "default": true
    },
    {
      "type": "header",
      "content": "t:sections.all.headers.header__1.content"
    },
    {
      "type": "select",
      "id": "color_scheme",
      "label": "t:sections.all.colors.settings.color_scheme.label",
      "options": [
        {
          "value": "color__bg-body color__text",
          "label": "t:sections.all.colors.settings.color_scheme.options__1.label"
        },
        {
          "value": "color__bg-neutral color__text",
          "label": "t:sections.all.colors.settings.color_scheme.options__2.label"
        },
        {
          "value": "color__bg-overlay-1 color__text",
          "label": "t:sections.all.colors.settings.color_scheme.options__3.label"
        },
        {
